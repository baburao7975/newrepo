pipeline{
    tools{
        jdk 'myjava'
        maven 'mymaven'
    }
	agent any
      stages{
           stage('gitclone'){
	    
               steps{
                 git 'https://github.com/baburao7975/pipelinecode.git'
              }
          }
          stage('Compile'){
             
              steps{

                  sh 'mvn compile'
	      }
          }
          
           stage('UnitTest'){
		  
              steps{
                  sh 'mvn test'
              }
             
           }	
          
          stage('Package'){
		  
              steps{
		  
                  sh 'mvn package'
              }
          }
	     
          
      }
}
